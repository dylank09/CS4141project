public class driver {
    public static void main(String[] args) {
        mainClassProj driver;                   // An Instance of mainClassProj is made and named "driver".
        driver = new mainClassProj();
        
        // Tests for the overloaded "move" functions
        driver.display();
        driver.move(3,6,true);
        driver.display();
        driver.move(5,2,false);
        driver.display();
        driver.move("Sheet2","Sheet6",false);
        driver.display();
        driver.move("Sheet7","Sheet3",true);
        driver.display();
    }
}
